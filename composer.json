{
    "name": "project/training",
    "description": "This is an advance training project",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Faker\\": "src/vendor/fzaninotto/faker/src/Faker/",
            "Twig\\" : "src/vendor/twig/twig/src/",
            "Doctrine\\DBAL\\": "src/vendor/doctrine/dbal/lib/Doctrine/DBAL" 
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Faker\\Test\\": "src/vendor/fzaninotto/faker/test/Faker/",
            "Twig\\Tests\\" : "src/vendor/twig/twig/tests/"
        }
    },
    "require": {
        "envms/fluentpdo": "^2.1",
        "fzaninotto/faker": "^1.9",
        "twig/twig": "^3.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "5",
        "phpunit/dbunit": "2"
    }
}
